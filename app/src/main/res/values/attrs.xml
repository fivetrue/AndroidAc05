<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmallItemView">
        <attr name="smallviewColor" format="color" />
        <attr name="topHeight" format="dimension" />
        <attr name="bottomHeight" format="dimension" />
        <attr name="width" format="dimension" />
        <attr name="margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DrawerButton">
        <attr name="android:padding" />
        <attr name="lineColor" format="color" />
        <attr name="lineCount" format="integer" />
        <attr name="lineWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="indicatorColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="dividerMargin" format="dimension" />
        <attr name="tabsPadding" format="dimension" />
        <attr name="tabsBackgroundColor" format="color" />
        <attr name="tabsTextSize" format="dimension" />
        <attr name="tabsTextColor" format="color" />
        <attr name="tabsTextAllCaps" format="boolean" />
        <attr name="tabsTextOutline" format="boolean" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="shouldExpand" format="boolean" />
    </declare-styleable>


</resources>